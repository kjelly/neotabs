{
  "background": {
    "service_worker": "background.js"
  },
  "description": "Another Tab Manager",
  "icons": {
    "128": "assets/tabs-128.png"
  },
  "minimum_chrome_version": "25",
  "manifest_version": 3,
  "offline_enabled": true,
  "name": "NeoTabs",
  "options_page": "",
  "permissions": [
    "tabs",
    "tabGroups",
    "system.memory",
    "contextMenus"
  ],
  "version": "2021.1.20",
  "commands": {
    "fullscreen": {
      "description": "fullscreen"
    },
    "left-tab": {
      "description": "left tab",
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    },
    "right-tab": {
      "description": "right tab"
    },
    "left-tab1": {
      "description": "left tab1"
    },
    "right-tab1": {
      "description": "right tab1"
    },
    "toggle-collapse": {
      "description": "collapse the group.",
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      }
    },
    "right-tab-skip-same-group": {
      "description": "right tab but skip the same group.",
      "suggested_key": {
        "default": "Ctrl+Shift+G",
        "mac": "Command+Shift+G"
      }
    },
    "close-tab": {
      "description": "close tab"
    },
    "new-tab": {
      "description": "new tab",
      "suggested_key": {
        "default": "Ctrl+Shift+N",
        "mac": "Command+Shift+N"
      }
    }
  }
}
